{
  "memory": {
    "name": "memory",
    "localStorage": "no",
    "file": "",
    "connector": "memory"
  },
  "sqlite": {
    "name": "sqlite",
    "connector": "loopback-connector-sqlite",
    "file_name": "./data.sqlite3",
    "debug": false
  },
  "files": {
    "name": "files",
    "connector": "loopback-component-storage",
    "provider": "filesystem",
    "nameConflict": "makeUnique",
    "root": "./server/files",
    "maxFileSize": 500000000
  },
  "transient": {
    "name": "transient",
    "connector": "transient"
  }
}
