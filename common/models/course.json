{
  "name": "Course",
  "plural": "courses",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "category": {
      "type": "string",
      "required": true
    },
    "abstract": {
      "type": "string"
    },
    "locked": {
      "type": "boolean",
      "default": false
     },
     "courseId": {
      "type": "string",
      "defaultFn": "uuidv4",
      "id": true
    },
    "lastChanged": {
      "type": "date",
      "default": "$now"
     },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "content": {
      "type": [
        "object"
      ],
      "default": []
    },
    "needsEnrollment": {
      "type": "boolean",
      "default": false
    },
    "storageId": {
      "type": "string"
    },
    "files": {
      "type": [
        "object"
      ]
    }
  },
  "validations": [],
  "relations": {
    "author": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": ""
    },
    "students": {
      "type": "hasMany",
      "model": "Account",
      "foreignKey": "",
      "through": "Enrollment"
    },
    "topics": {
      "type": "hasMany",
      "model": "CourseTopic",
      "foreignKey": ""
    },
    "quizzes": {
      "type": "hasMany",
      "model": "CourseQuiz",
      "foreignKey": ""
    },
    "startInteraction": {
      "type": "belongsTo",
      "model": "Learninteraction"
    },
    "flags": {
      "type": "hasMany",
      "model": "Flag"
    }
  },
  "acls": [],
  "methods": {}
}
